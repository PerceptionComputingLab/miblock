{
  "img_dir": "W:\\LITS17\\image",
  "lab_dir": "W:\\LITS17\\label",
  "mode": "train",
  "epochs": 20,
  "data_process_pipeline": [
    { "type": "LoadImage" },
    {
      "type": "RandomCrop",
      "crop_size": 16
    }
  ],
  "loader": {
    "batch_size": 2,
    "num_workers": 1
  },
  "model": {
    "type": "UNet",
    "in_channel": 1,
    "out_channel": 1
  },
  "loss": {
    "type": "DiceLoss"
  },
  "optimizer": {
    "type": "SGD",
    "lr": 0.1
  }
}